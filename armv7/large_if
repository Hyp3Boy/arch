		;R0=f,	R1=g, R2=h, R3=i, R4=j
		MOV		R1, #5 ;
		MOV		R2, #2 ;
		MOV		R3, #4 ;
		MOV		R4, #4 ;
		CMP		R3, R4 ; set flags with R3-R4
		BNE		L1 ; if i!=j, salta al branch L1, o sea si el flag ZERO(Z) no esta activado :)
		ADD		R0, R1, R2 ; f = g + h
		B		exit ; como sabemos que i==j entonces despu√©s de ejecutar la suma, salte al branch exit que termina el programa
L1
		SUB		R0, R0, R2 ; f = f - i
exit
